{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-15.1.0_20250607/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc",
      "compileCommands": "${config:idf.buildPath}/compile_commands.json",
      "includePath": [
        "${config:idf.espIdfPath}/components/**",
        "${config:idf.espIdfPathWin}/components/**",
        "${workspaceFolder}/**",
        "${workspaceFolder}/build/config",
        "${workspaceFolder}/main",
        "${workspaceFolder}/main/wifi_station",
        "${workspaceFolder}/main/ws_client",
        "${workspaceFolder}/main/motor_control",
        "${workspaceFolder}/main/autonomous_task",
        "${workspaceFolder}/managed_components/**",
        "${config:idf.buildPath}/config"
      ],
      "browse": {
        "path": [
          "${config:idf.espIdfPath}/components",
          "${config:idf.espIdfPathWin}/components",
          "${workspaceFolder}",
          "${workspaceFolder}/main",
          "${workspaceFolder}/main/wifi_station",
          "${workspaceFolder}/main/ws_client",
          "${workspaceFolder}/main/motor_control",
          "${workspaceFolder}/main/autonomous_task",
          "${workspaceFolder}/managed_components",
          "${workspaceFolder}/build/config"
        ],
        "limitSymbolsToIncludedHeaders": false
      },
      "defines": [
        "ESP_PLATFORM",
        "__XTENSA__",
        "CONFIG_FREERTOS_HZ=1000",
        "portTICK_PERIOD_MS=1"
      ],
      "forcedInclude": ["${workspaceFolder}/build/config/sdkconfig.h"],
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x64"
    }
  ],
  "version": 4
}
